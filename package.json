
{
  "name": "discord-bot-railway-starter-ts",
  "version": "1.0.0",
  "private": true,
  "engines": { "node": ">=20" },
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "register:dev": "ts-node src/register-commands.ts dev",
    "register:global": "ts-node src/register-commands.ts global"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
